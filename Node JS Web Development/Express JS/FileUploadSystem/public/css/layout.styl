body 
    div#cssmenu 
        width: 960px
        font-family Verdana Geneva 'DejaVu Sans' sans-serif
        font-size 14px
        margin 20px 7px

        ul 
            list-style-type none
            background-color #CFCFCF
            border-bottom 5px solid #0079B2

            li
                height 20px
                background-color #0079b2
                margin-top -18px
                display inline-block
                width 110px
                height 20px
                padding 10px 5px
                text-align center
                position relative
                text-decoration none
                color #5D6E66
                font-weight bold
                a 
                    color #fff
                
                a:hover
                    background-color #0079b2
    footer
        text-align center
        padding 10px